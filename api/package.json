{
  "name": "musixer-api",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "license": "MIT",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.2.16",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^16.11.10",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.22",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@types/musicmatch": "^0.3.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.1",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cros": "^1.0.1",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.17.2",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.0",
    "musicmatch": "^0.3.2",
    "pg": "^8.4.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "swagger-autogen": "^2.23.2",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "0.3.17",
    "winston": "^3.9.0",
    "zod": "^3.21.4"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon src/main.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "build": "tsc -p .",
    "migration:create": "npm run typeorm migration:create ./src/migration -- -d ./src/data-source.ts",
    "migrate": "rm -rf build && npm run build && npm run typeorm migration:generate ./src/migration/added-entity -- -d ./src/data-source.ts",
    "db:push": "rm -rf build && npm run build && npm run typeorm migration:run -- -d src/data-source.ts",
    "test": "jest",
    "swagger": "ts-node ./src/docs/swagger.ts",
    "fly:deploy": "fly deploy",
    "fly:swagger": "node ./build/src/docs/swagger.js"
  }
}
